{
  "name": "nextjs-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "eslint-check": "eslint --print-config path/to/main.js | eslint-config-prettier-check",
    "format": "prettier --write ./**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint \"**/*.ts\" \"**/*.tsx\" --fix",
    "pre-commit": "npm run lint",
    "prepare": "husky install",
    "dev:prod-env": "NODE_ENV=prod next dev",
    "dev:dev-env": "NODE_ENV=dev next dev",
    "build:prod-env": "NODE_ENV=prod next build",
    "build:dev-env": "NODE_ENV=dev next build"
  },
  "dependencies": {
    "@craco/craco": "^6.4.3",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@gokiprotocol/walletkit": "^1.7.3",
    "@hamsterbox/ui-kit": "^0.3.5",
    "@reach/dialog": "^0.17.0",
    "@reduxjs/toolkit": "^1.8.0",
    "@saberhq/solana-contrib": "^1.13.19",
    "@saberhq/token-utils": "^1.13.19",
    "@saberhq/use-solana": "^1.13.19",
    "@solana/web3.js": "^1.43.2",
    "@types/bn.js": "^5.1.0",
    "@types/react-dom": "^18.0.9",
    "antd": "^5.0.0",
    "axios": "^1.0.0",
    "bn.js": "^5.2.1",
    "buffer": "^6.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.3",
    "husky": "^8.0.1",
    "jsbi": "^4.3.0",
    "next": "^12.3.1",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.2.1",
    "next-transpile-modules": "^9.1.0",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.6",
    "react-scripts": "5.0.1",
    "redux": "^4.1.2",
    "redux-saga": "^1.1.3",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/node": "17.0.21",
    "@types/react": "^18.0.25",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "autoprefixer": "^10.4.2",
    "bs58": "^5.0.0",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "husky": "^8.0.0",
    "postcss": "^8.4.8",
    "prettier": "^2.7.1",
    "sass": "^1.49.9",
    "tailwindcss": "^3.0.23",
    "typescript": "4.6.2"
  }
}
